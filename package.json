{
  "name": "enapter-blueprints-ide",
  "displayName": "enapter-blueprints-ide",
  "description": "Enapter Blueprints IDE",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "enapter-blueprints",
          "title": "Enapter Blueprints",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "enapter-blueprints": [
        {
          "id": "enapter-blueprints.views.upload",
          "name": "Upload Blueprint"
        },
        {
          "id": "enapter-blueprints.views.devices.recent",
          "name": "Previously Used Devices"
        },
        {
          "id": "enapter-blueprints.views.devices.active",
          "type": "webview",
          "name": "Active Device"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "enapter-blueprints.views.upload",
        "contents": "Upload a Blueprint\n[Upload](command:enapter-blueprints.commands.uploadBlueprint)"
      }
    ],
    "commands": [
      {
        "command": "enapter-blueprints.commands.uploadBlueprint",
        "title": "Upload Blueprint"
      },
      {
        "command": "enapter-blueprints.commands.devicesRecentRefresh",
        "title": "Refresh Recent Device IDs"
      }
    ],
    "configuration": {
      "title": "Enapter",
      "type": "object",
      "properties": {
        "enapter.apiKey": {
          "type": "string",
          "default": "",
          "description": "Enapter API Key"
        },
        "enapter.apiHost": {
          "type": "string",
          "default": "https://api.enapter.com",
          "description": "Enapter API Host"
        }
      }
    }
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/extensionTests.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "npm run check-types && npm run lint && node esbuild.js",
    "watch-web": "npm-run-all -p watch-web:*",
    "watch-web:esbuild": "node esbuild.js --watch",
    "watch-web:webview:esbuild": "node esbuild.webview.js --watch",
    "watch-web:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package-web": "npm run check-types && npm run lint && node esbuild.js --production",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@types/assert": "^1.5.10",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.10.1",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-web": "^0.0.63",
    "assert": "^2.1.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "glob": "^11.0.0",
    "mocha": "^10.7.3",
    "npm-run-all": "^4.1.5",
    "prettier": "3.4.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "wretch": "^2.11.0"
  },
  "bundleDependencies": [
    "reflect-metadata"
  ]
}
